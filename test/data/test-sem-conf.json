{
  "configuration": {
    "env": {
      "params": {
        "app.name": "test-sem-app"
      },
      "logging": {
        "level": "debug",
        "size": "5M",
        "max-files": "3",
        "mode": "debug",
        "console": "true"
      }
    },
    "control": {
      "def": [
        {
          "resource": {
            "name": "dummy-resource-1",
            "class": "com::watergate::core::dummy_resource",
            "max_concurrency": "8"
          },
          "priorities": "2"
        },
        {
          "resource": {
            "name": "fs-resource-0",
            "class": "com::watergate::core::io::filesystem_driver",
            "params": {
              "fs.path.root": "/tmp",
              "fs.quota.bytes": "5000",
              "fs.quota.lease.time": "500",
              "fs.concurrency.max": "8"
            }
          },
          "priorities": "3"
        }
      ],
      "manager": {
        "lock.reset.time": "5s",
        "record.reset.time": "30s"
      }
    },
    "test-lock-client": {
      "process": "/work/dev/wookler/watergate/test/cmake/test_lock_client",
      "process.count": "30",
      "sleep.time": "30",
      "lock.tries": "100"
    }
  }
}